package com.dsa.stackDemo;

public class Stack {
//	Stack(int n){
//		this.size=n;
//	}
//	int size;
	int[] stack = new int[5];
	int top=0;
	public void push(int data) {
		stack[top]=data;
		top++;
	}
	
	public int pop() {
		return stack[top--];
	}
	
	public void show() {
		for(int n : stack) {
			System.out.print(n+" ");
		}
		System.out.println();
	}
}
