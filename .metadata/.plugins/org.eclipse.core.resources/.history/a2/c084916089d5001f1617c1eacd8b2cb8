package com.dsa.datastructures.stack.linkedlist;

public class Stack {
	Node top;
	
	//check if stack is empty
	public boolean isEmpty() {
		return top==null;
	}
	
	//add elements in the stack: push operation
	public void push(int data) {
		Node node = new Node();
		node.data=data;
		if(node==null) {
			System.out.println("Stack overflow");
		}
	}
}
